      "before": ["<leader>", "P", "n"]
    },
    {
      "after": ["a", ":", "g", "r", "e", "y", "_", "q", "u", "e", "s", "t", "i", "o", "n", ":", "<Esc>", "F", ":", "h"],
      "before": ["<leader>", "p", "q"]
    },
    {
      "after": ["i", ":", "g", "r", "e", "y", "_", "q", "u", "e", "s", "t", "i", "o", "n", ":", "<Esc>", "F", ":", "h"],
      "before": ["<leader>", "P", "q"]
    },
    {
      "after": ["a", ":", "x", ":", "<Esc>", "F", ":", "h"],
