{
  "C_Cpp.clang_format_sortIncludes": true,
  "C_Cpp.formatting": "vcFormat",
  "C_Cpp.updateChannel": "Insiders",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyType": true,
  "C_Cpp.vcFormat.space.beforeEmptySquareBrackets": true,
  "C_Cpp.vcFormat.space.beforeInitializerListOpenBrace": true,
  "C_Cpp.vcFormat.space.betweenEmptyBraces": true,
  "C_Cpp.vcFormat.space.betweenEmptyLambdaBrackets": true,
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "editor.semanticHighlighting.enabled": true,
    "editor.suggest.insertMode": "replace",
    "editor.wordBasedSuggestions": false
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 4,
    "editor.wordWrap": "on",
    "editor.wrappingIndent": "indent"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "autoDocstring.quoteStyle": "'''",
  "breadcrumbs.enabled": false,
  "cSpell.enabled": false,
  "cSpell.userWords": [
    "appl",
    "datetime",
    "discoverability",
    "dualog",
    "elem",
    "Fogg",
    "Foggs",
    "gamification",
    "inkbar",
    "interpretivism",
    "leaderboard",
    "leaderboards",
    "mypage",
    "ngneat",
    "Precommitment",
    "Sunstein",
    "Webmail"
  ],
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.wordWrap": "on",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.cursorBlinking": "solid",
  "editor.detectIndentation": false,
  "editor.fontSize": 13,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.hover.delay": 500,
  "editor.hover.sticky": false,
  "editor.insertSpaces": false,
  "editor.lightbulb.enabled": false,
  "editor.lineHeight": 0,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.mouseWheelZoom": true,
  "editor.renderWhitespace": "trailing",
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "deepIndent",
  "editor.wrappingStrategy": "advanced",
  "files.associations": {
    "*.bash": "shellscript",
    "*.h": "c",
    "*.latex": "latex"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.build": true,
    "**/*.hex": true,
    "**/*.bin": true,
    "keyboards": false,
    "keyboards/[^p]*": true,
    "keyboards/?[^l]*": true,
    "keyboards/??[^a]*": true,
    "keyboards/???[^n]*": true,
    "keyboards/planck/[^(r|k)]*": true,
    "keyboards/planck/?[^e]*": true,
    "keyboards/planck/??[^(v|y)]*": true,
    "keyboards/planck/???[^(5|m)]*": true,
    "keyboards/planck/keymaps/[^p]*": true,
    "keyboards/planck/keymaps/?[^r]*": true,
    "keyboards/planck/keymaps/??[^o]*": true
  },
  "githubPullRequests.fileListLayout": "tree",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "latex-workshop.view.pdf.invertMode.brightness": 0.9,
  "latex-workshop.view.pdf.invertMode.grayscale": 0.8,
  "latex-workshop.view.pdf.viewer": "tab",
  "markdown.extension.list.indentationSize": "inherit",
  "markdown.preview.scrollEditorWithPreview": false,
  "markdown.preview.scrollPreviewWithEditor": false,
  "markdownlint.config": {
    "header-style": false,
    "list-marker-space": false,
    "no-duplicate-header": false,
    "no-inline-html": false,
    "single-h1": false,
    "ul-indent": false
  },
  "omnisharp.organizeImportsOnFormat": true,
  "terminal.integrated.fontSize": 17,
  "todo-tree.general.tagGroups": {
    "TEMP": ["TEMP", "TMP"]
  },
  "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "XXX", "TEMP", "TMP", "[ ]", "[x]"],
  "todo-tree.highlights.customHighlight": {
    "BUG": {
      "background": "black",
      "foreground": "orange",
      "icon": "bug"
    },
    "FIXME": {
      "background": "black",
      "foreground": "red",
      "icon": "flame"
    },
    "TEMP": {
      "background": "black",
      "foreground": "magenta",
      "icon": "x"
    },
    "TODO": {
      "background": "black",
      "foreground": "cyan",
      "icon": "check",
      "iconColor": "green"
    }
  },
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.replace": "block",
  "vim.cursorStylePerMode.visual": "block",
  "vim.handleKeys": {
    "<C-b>": false,
    "<C-j>": false,
    "<C-k>": false
  },
  "vim.leader": " ",
